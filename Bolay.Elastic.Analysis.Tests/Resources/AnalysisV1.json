{
   "tokenizer":{
      "semicolon_token":{
         "type":"pattern",
         "pattern":";"
      },
      "hashsign_token":{
         "type":"pattern",
         "pattern":"#"
      }
   },
   "analyzer":{
      "semicolon_analyzer":{
         "type":"custom",
         "tokenizer":"semicolon_token"
      },
      "hashsign_analyzer":{
         "type":"custom",
         "tokenizer":"hashsign_token"
      },
      "standard_unaccent":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "accent_folding"
         ]
      },
      "standard_edge_ngram":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "edge_ngram"
         ]
      },
      "edge_ngram_wildcard":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "edge_ngram_front_one_to_thirtytwo",
            "edge_ngram_back_one_to_thirtytwo"
         ]
      },
      "ngram_disambiguation":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "edge_ngram"
         ]
      },
      "edge_ngram_disambiguation_front":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "edge_ngram_front_two_to_four"
         ]
      },
      "edge_ngram_disambiguation_back":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "edge_ngram_back_two_to_four"
         ]
      },
      "postal_code":{
         "tokenizer":"keyword",
         "filter":[
            "standard",
            "lowercase",
            "whitespace_remover"
         ]
      },
      "postal_code_edge_ngram":{
         "tokenizer":"keyword",
         "filter":[
            "standard",
            "lowercase",
            "whitespace_remover",
            "edge_ngram"
         ]
      },
      "standard_stemming":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "stemming"
         ]
      },
      "standard_phonetic":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "phonetic"
         ]
      },
      "metaphone_phonetic":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "phonetic_metaphone"
         ]
      },
      "keyword":{
         "tokenizer":"keyword",
         "filter":[
            "standard"
         ]
      },
      "keyword_lowercase":{
         "tokenizer":"keyword",
         "filter":[
            "lowercase"
         ]
      },
      "synonym_name":{
         "tokenizer":"standard",
         "filter":[
            "synonym_name"
         ]
      },
      "synonym_address":{
         "tokenizer":"standard",
         "filter":[
            "synonym_address"
         ]
      },
      "flexible":{
         "tokenizer":"standard",
         "filter":[
            "word_delimiter",
            "accent_folding",
            "stop_words"
         ]
      },
      "standard_ngram":{
         "tokenizer":"standard",
         "filter":[
            "standard",
            "lowercase",
            "stop",
            "ngram"
         ]
      },
      "icu_autocomplete":{
         "type":"custom",
         "tokenizer":"icu_tokenizer",
         "filter":[
            "icu_folding",
            "edge_ngram"
         ]
      }
   },
   "filter":{
      "whitespace_remover":{
         "type":"pattern_replace",
         "pattern":" ",
         "replacement":""
      },
      "edge_ngram":{
         "type":"edgeNGram",
         "side":"front",
         "min_gram":1,
         "max_gram":32
      },
      "edge_ngram_front_one_to_thirtytwo":{
         "type":"edgeNGram",
         "side":"front",
         "min_gram":1,
         "max_gram":32
      },
      "edge_ngram_back_one_to_thirtytwo":{
         "type":"edgeNGram",
         "side":"back",
         "min_gram":1,
         "max_gram":32
      },
      "ngram_two_to_four":{
         "type":"ngram",
         "min_gram":2,
         "max_gram":4
      },
      "edge_ngram_front_two_to_four":{
         "type":"edgeNGram",
         "side":"front",
         "min_gram":2,
         "max_gram":4
      },
      "edge_ngram_back_two_to_four":{
         "type":"edgeNGram",
         "side":"back",
         "min_gram":2,
         "max_gram":4
      },
      "ngram":{
         "type":"ngram",
         "min_gram":3,
         "max_gram":3
      },
      "stemming":{
         "type":"stemmer",
         "name":"minimal_english"
      },
      "phonetic":{
         "type":"phonetic",
         "name":"refined_soundex"
      },
      "phonetic_metaphone":{
         "type":"phonetic",
         "encoder":"metaphone"
      },
      "synonym_name":{
         "type":"synonym",
         "synonyms_path":"analysis/synonym_name.txt"
      },
      "synonym_address":{
         "type":"synonym",
         "synonyms_path":"analysis/synonym_address.txt"
      },
      "word_delimiter":{
         "type":"word_delimiter",
         "split_on_numerics":"false",
         "stem_english_possessive":"false"
      },
      "accent_folding":{
         "type":"icu_folding",
         "replace":"false"
      },
      "stop_words":{
         "type":"stop",
         "stopwords_path":"analysis/stopwords.txt"
      }
   }
}